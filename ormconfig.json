[
    {
        "name": "unit",
        "type": "sqlite",
        "database": ":memory:",
        "synchronize": true,
        "logging": false,
        "autoSchemaSync": true,
        "entities": [
            "src/entity/**/*.ts"
        ],
        "subscribers": [
            "src/subscriber/*.ts"
        ],
        "migrations": [
            "src/migration/*.ts"
        ],
        "cli": {
            "entitiesDir": "src/entity/**/*.ts",
            "migrationsDir": "src/migration/**/*.ts",
            "subscribersDir": "src/subscriber/**/*.ts"
        }
    }
]