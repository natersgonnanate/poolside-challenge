  "dependencies": {
    "@types/compression": "^1.0.1",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/express-openapi": "^0.11.33",
    "@types/node": "^12.11.1",
    "@types/request-promise": "^4.1.44",
